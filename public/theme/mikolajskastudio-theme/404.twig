{% extends 'modules/_lang_detect.twig' %}
{% block main %}
    {% setcontent resources_title = resourcecontenttype ~ '/tytul' %}
<html>
<head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <!-- Mobile-friendly viewport -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Style sheet link -->
    <link href="{{ paths.theme }}stylesheets/style.css" rel="stylesheet" media="all">
    {#<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css">#}
    {% if language == 'pl' %}
        <title>Błąd 404 | {{ resources_title.html }} </title>
    {% elseif language == 'en' %}
        <title>Error 404 | {{ resources_title.html }} </title>
    {% endif %}
</head>
<body>
        
        <div class="page error">
            <div class="error-wrapper">
            <img src="{{ paths.theme }}images/404.png">
                        {% if language == 'pl' %}
                            <a href="{{ paths.rooturl }}"><div class="back-button">POWRÓT DO STRONY GŁÓWNEJ</div></a>
                        {% elseif language == 'en' %}
                            <a href="{{ paths.rooturl }}en/mikolajska-studio"><div class="back-button">BACK TO HOMEPAGE</div></a>
                        {% endif %}
            </div>
        </div>
        {% include 'modules/_footer2.twig' %}
</body>
</html>
{% endblock main %}